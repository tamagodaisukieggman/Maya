                        string $elm;

                        for($elm in $parentMaterials)
                        {
                            menuItem -l $elm;

                            // 最後に選択していた親マテリアルを選択
                            if($elm == $last_selected_item)
                            {
                                optionMenu -e -v $elm uOm_parentMaterial_shr_exportFbx;
                            }
                        }

                        checkBox
                            -l "再設定"
                            -ann "マテリアルインスタンスが既に存在する場合に、親マテリアルを再設定します"
                            -v `optionVar -q ov_resetParentMaterial_shr_exportFbx`
                            -onc "optionVar -iv ov_resetParentMaterial_shr_exportFbx 1"
                            -ofc "optionVar -iv ov_resetParentMaterial_shr_exportFbx 0"
                            uCb_resetParentMaterial_shr_exportFbx;

                        text -l "物理マテリアル : ";

                        // 物理マテリアルプルダウンメニュー
                        optionMenu
                        -ann "マテリアルインスタンスに設定する物理マテリアル"
                        -cc "optionVar -sv ov_physMaterial_shr_exportFbx #1"
                        uOm_physMaterial_shr_exportFbx;

                        // 最後に選択していた物理マテリアル
                        $last_selected_item = `optionVar -q "ov_physMaterial_shr_exportFbx"`;

                        // 物理マテリアルリストを取得
                        string $physMaterials[] = shr_exportFbx_getPhysMaterials();

                        for($elm in $physMaterials)
                        {
                            menuItem -l $elm -ann "testdfdfd";

                            // 最後に選択していた物理マテリアルを選択
                            if($elm == $last_selected_item)
                            {
                                optionMenu -e -v $elm uOm_physMaterial_shr_exportFbx;
                            }
                        }

                        checkBox
                            -l "再設定"
                            -ann "マテリアルインスタンスが既に存在する場合に、物理マテリアルを再設定します"
                            -v `optionVar -q ov_resetPhysMaterial_shr_exportFbx`
                            -onc "optionVar -iv ov_resetPhysMaterial_shr_exportFbx 1"
                            -ofc "optionVar -iv ov_resetPhysMaterial_shr_exportFbx 0"
                            uCb_resetPhysMaterial_shr_exportFbx;

                        text -l "再アサイン : " -ann "マテリアルインスタンスが既に存在する場合に再アサインをするか";

                        radioButtonGrp
                            -nrb 2
                            -la2 "しない" "する"
                            -an1 "マテリアルインスタンスの再アサインをスキップします"
                            -an2 "マテリアルインスタンスを規約に沿って再アサインします"
                            -cw 1 56
                            -sl `optionVar -q "ov_reassignMaterial_shr_exportFbx"`
                            -cc1 "shr_exportFbx_onRadioButtonChanged(1)"
                            -cc2 "shr_exportFbx_onRadioButtonChanged(2)"
                            uRbg_reassignMaterial_shr_exportFbx;

                        setParent ..;
                    setParent ..;

                separator -st "in" -h 8;

                columnLayout -adj on;
                    checkBox
                        -l "コレクションに登録"
                        -ann "UE5 にインポート後、共有コレクションに登録します"
                        -v `optionVar -q ov_addToCollections_shr_exportFbx`
                        -cc "shr_exportFbx_onAddToCollectionsCheckboxChanged"
                        uCb_addCollection_shr_exportFbx;

                        setParent ..;

                columnLayout -adj on -co "left" 16;
                    frameLayout
                        -lv off
                        -en (`optionVar -q ov_addToCollections_shr_exportFbx`)
                        uFL_collectionList_shr_exportFbx;

                        textScrollList
                            -h 50
                            -allowMultiSelection on
                            uTsl_collections_shr_exportFbx;

                        setParent ..;
                    setParent ..;

                setParent ..;
            setParent ..;

        separator -st "in";

        rowLayout -nc 2 -adj 1;

            button
                -l "FBX 出力"
                -c "shr_exportFbx_onExportButtonCallback"
                -bgc 0.5 0.6 0.5
                ;

            symbolButton
                -image "fileOpen.png"
                -ann "FBX 出力先フォルダをエクスプローラで開く"
                -c "shr_exportFbx_openInExplorer"
                ;

            setParent ..;

    // リスト更新
    shr_exportFbx_updateAssetList;
    shr_exportFbx_updateCollectionList;

    // シーンオープン時にリストを更新
