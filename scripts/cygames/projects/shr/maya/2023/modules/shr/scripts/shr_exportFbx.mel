/////////////////////////////////////////////////////////////////
// UI 作成
/////////////////////////////////////////////////////////////////
global proc shr_exportFbx()
{
    // TA ツールログ送信
    shr_toolLog("Environment Exporter", "", "launched.");

    source "shr_p4Cmds";
    source "shr_fixHierarchy";
    source "shr_collisionUtils";
    source "shr_ueRemoteClient";

    // fbxmaya プラグインが無効なら有効化
    shr_exportFbx_checkFbxmayaPlugin;

    //プリファレンスに値が無ければデフォルト値を設定
    if (`optionVar -ex "ov_parentMaterial_shr_exportFbx"` == 0)
    {
        optionVar -sv "ov_parentMaterial_shr_exportFbx" "mi_std01_01_base";
    }
    if (`optionVar -ex "ov_reassignMaterial_shr_exportFbx"` == 0)
    {
        optionVar -iv "ov_reassignMaterial_shr_exportFbx" 1;
    }
    if (`optionVar -ex "ov_enableNanite_shr_exportFbx"` == 0)
    {
        optionVar -iv "ov_enableNanite_shr_exportFbx" 1;
    }
    if (`optionVar -ex "ov_addToCollections_shr_exportFbx"` == 0)
    {
        optionVar -iv "ov_addToCollections_shr_exportFbx" 1;
    }

    // HACK: 2022/12/16 グループノードを介さず階層化された状態だと、UE5 に正しくインポートされない
    // UDN 問い合わせ中
    // 暫定対応としてマージするオプションを追加
    if (`optionVar -ex "ov_mergeMeshes_shr_exportFbx"` == 0)
    {
        optionVar -iv "ov_mergeMeshes_shr_exportFbx" 0;
    }

    menuBarLayout;

    // メニュー
    menu -label "リスト";
        menuItem
            -label "リストを更新"
            -i "refresh.png"
            -c "shr_exportFbx_updateAssetList; shr_exportFbx_updateCollectionList"
            ;

    menu -label "ツール";
        menuItem
            -label "選択アセットの旧仕様の階層構造を修正..."
            -c "shr_exportFbx_fixHierarchyMenuCallback()"
            ;

        menuItem -d true;
        menuItem
            -label "選択アセットのコリジョン用メッシュノードをリネーム"
            -c "shr_exportFbx_renameAllCollisionNodesMenuCallback()"
            ;

    // HACK: 2022/12/16 グループノードを介さず階層化された状態だと、UE5 に正しくインポートされない
    // UDN 問い合わせ中
    // 暫定対応としてマージするオプションを追加
    int $mergeMesh = `optionVar -q ov_mergeMeshes_shr_exportFbx`;

    menu -label "設定";
        menuItem
            -label "表示メッシュをマージしてFBX 出力"
            -cb $mergeMesh
            -c ("optionVar -iv ov_mergeMeshes_shr_exportFbx " + ($mergeMesh != 1))
            ;

    menu -l "ヘルプ" -helpMenu on;
        menuItem
            -l "物理マテリアル一覧"
            -c "showHelp -absolute \"https://wisdom.cygames.jp/pages/viewpage.action?pageId=539930136#Env:%E3%83%AF%E3%83%BC%E3%82%AF%E3%83%95%E3%83%AD%E3%83%BC%E3%83%9E%E3%83%86%E3%83%AA%E3%82%A2%E3%83%AB%E7%B4%A0%E6%9D%90%E3%82%B5%E3%82%A6%E3%83%B3%E3%83%89/%E3%82%A8%E3%83%95%E3%82%A7%E3%82%AF%E3%83%88%E7%B4%A0%E6%9D%90%E4%B8%80%E8%A6%A7-%E3%83%9E%E3%83%86%E3%83%AA%E3%82%A2%E3%83%AB%E4%B8%80%E8%A6%A7\""
            ;

        menuItem -d true;

        menuItem
            -l "Environment Exporter : 背景用FBX 書き出しツールのヘルプ"
            -i "help.png"
            -c "showHelp -absolute \"https://wisdom.cygames.jp/pages/viewpage.action?pageId=452511931\""
