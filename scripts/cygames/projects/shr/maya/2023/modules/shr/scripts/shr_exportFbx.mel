global proc int shr_exportFbx_addAssetToCollections(string $assetPath, string $collections[])
{
    // jon_info を設定
    string $jobInfoList[];
    $jobInfoList = shr_ueRemoteClient_appendStrValueToJobInfoList($jobInfoList, "asset_path", $assetPath);
    $jobInfoList = shr_ueRemoteClient_appendStrArrayToJobInfoList($jobInfoList, "collections", $collections);

    // リモート実行結果
    string $remoteResult[] = shr_ueRemoteClient_runFile("add_asset_to_collections", $jobInfoList, 0);

    // 成否
    return shr_ueRemoteClient_isSuccess($remoteResult);
}

