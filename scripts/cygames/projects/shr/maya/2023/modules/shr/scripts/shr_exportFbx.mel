global proc string[] shr_exportFbx_getAllTargetNodes(string $allNodes[])
{
    string $TargetNodes[];

    string $elm;

    for($elm in $allNodes)
    {
        // 正しいルートノード名の場合
        if(`shr_fixHierarchy_checkRootNodeName($elm)` == 1)
